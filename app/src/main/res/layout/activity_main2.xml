<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.willow.lab2.Main2Activity">

    <ImageButton
        android:id="@+id/supermoon"
        android:layout_width="113dp"
        android:layout_height="237dp"
        android:layout_marginBottom="207dp"
        android:layout_marginEnd="59dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="67dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/waterfall"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/supermoon" />

    <ImageButton
        android:id="@+id/waterfall"
        android:layout_width="113dp"
        android:layout_height="237dp"
        android:layout_marginBottom="207dp"
        android:layout_marginEnd="53dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="67dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/supermoon"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/waterfall" />
</android.support.constraint.ConstraintLayout>
